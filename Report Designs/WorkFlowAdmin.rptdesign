                        <method name="clientScripts"><![CDATA[
/**
 * Occurs when mouse clicks.
 * @param event
 */

this.onclick = function(event)
{ 
	var recIds = "0";
	$('[id^="ckBox|"]').each(function (index, element) {
		if (element.checked) {
			var arrBoxId = element.id.split("|");
			recIds = recIds + "," + arrBoxId[2];
		}
	});
	if (recIds != "0") {
		var url = "/iportal/executereport.do?__executableName=";
		url = url + encodeURIComponent(this.getViewer().getReportName());
		//url = url + "&amp;pReportName=" + encodeURIComponent(pReportName);
		url = url + "&amp;pKey=" + encodeURIComponent(recIds);
		url = url + "&amp;invokesubmit=true&amp;__wait=wait&amp;__requestType=immediate"; 
		url = url + "&amp;pUpdateStatus=Delete";
		//url = url + "&amp;pShortComment=" + shortComment;
		//url = url + "&amp;pUpdateComment=" + encodeURIComponent(Comment);
		//location.href = url;
		var vwr = this.getViewer();
		vwr.setParameters({pKey:recIds,pUpdateStatus:'Delete'});
		vwr.submit();
	}
	else
		alert("Nothing selected.");
}
]]></method>
